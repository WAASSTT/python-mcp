    "elysia": "^1.1.29",
    "groq-sdk": "^0.8.0",
    "ioredis": "^5.4.1",
    "lunar-typescript": "^1.7.5",
    "nanoid": "^5.0.9",
    "openai": "^4.75.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
